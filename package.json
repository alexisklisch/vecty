{
  "name": "vecty",
  "version": "0.0.19",
  "description": "Add variables to XML files.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/main.js",
  "module": "dist/mjs/main.js",
  "files": ["dist"],
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/main.js",
