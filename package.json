      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "test": "vitest",
    "temp": "tsx test/temp.ts",
    "build": "tsc",
    "build:rs": "rspack build && tsc && tsc-alias"
  },
  "keywords": [],
  "author": "Alexis Fleitas Klisch",
  "license": "MIT",
  "devDependencies": {
    "@rspack/cli": "1.3.2",
    "@rspack/core": "1.3.2",
    "@types/node": "22.13.17",
    "@types/opentype.js": "1.3.8",
    "tsc-alias": "1.8.13",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vitest": "3.1.1"
  },
  "packageManager": "pnpm@10.7.0"
}